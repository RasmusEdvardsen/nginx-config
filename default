server {
	listen 80;
	
	location /portfolio {
		proxy_pass http://127.0.0.1:23001/;
	}

	location /storage-fe {
		root /home/build;
		try_files $uri $uri/ =404;
	}

	location ~* \.(js|jpg|png|css)$ {
		proxy_pass http://127.0.0.1:23001;
	        #root /home/build/storage-fe;
        	#expires 30d;
    	}
}
